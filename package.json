{
  "name": "proxy",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon --watch src --watch .env --watch '*.json' --ext ts,js,json,env --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "build": "tsc && tsc-alias",
    "watch": "tsc -w"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
